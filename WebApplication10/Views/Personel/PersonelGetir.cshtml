@model MVC5_3_LAYERS_PROJECT.Models.Entity.Table_Personel
@{
    ViewBag.Title = "PersonelGetir";
    Layout = "~/Views/Shared/layout.cshtml";
}

@using (Html.BeginForm("PersonelGuncelle", "Personel", FormMethod.Post))
{
    <div class="form-group">
        @Html.LabelFor(p => p.ID)
        @Html.TextBoxFor(p => p.ID, new { @class = "form-control" })
        <br />
        @Html.LabelFor(p => p.PERSONEL)
        @Html.TextBoxFor(p => p.PERSONEL, new { @class = "form-control" })
    </div>
    <div>
        <button class="btn btn-outline-success">Guncelle</button>
    </div>
}

